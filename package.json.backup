    "babel-preset-react": "*",
    "babel": "*",
    "babel-core": "*",
    "babel-loader": "*",


    "react": "*",
    "react-dom": "*",

"scripts": {
  "setup": "git clean -xdf && npm install && npm run typings",
  "update": "git fetch && git merge origin master && npm run setup",
  "typings": "tsd reinstall && tsd rebundle",
  "validate": "npm ls",
  "clean": "rm -rf es5 es6 coverage",
  "compile:es6": "find src test bench typings -name \"*.ts\" | xargs tsc --declaration --target es6 --noImplicitAny --sourceMap --outDir es6",
  "compile:es5": "find src test bench typings -name \"*.ts\" | xargs tsc --declaration --target es5 --module commonjs --noImplicitAny --sourceMap --outDir es5",
  "compile:test": "find src test bench typings -name \"*.ts\" | xargs tsc --target es5 --module commonjs --noImplicitAny --sourceMap --outDir es5",
  "compile:base": "npm run --silent compile:es5",
  "precompile": "npm run --silent clean",
  "compile": "npm run --silent compile:base",
  "coveralls": "cat ./coverage/lcov.info | coveralls",
  "lint": "find src test bench -name \"*.ts\" | xargs tslint",
  "lint:silent": "npm run --silent lint || echo '\n\n'",
  "test:base": "istanbul test tap \"es5/**/*.test.js\" --ui tdd",
  "test:cover": "istanbul cover tape \"es5/**/*.test.js\" && istanbul report --include=./coverage clover",
  "test:silent": "npm run --silent test:base || echo '\n\n'",
  "pretest": "npm run compile:test && find es5 -type f -name *.js -exec sed -i'.bak' -e '1s/^var __extends/\\/\\* istanbul ignore next \\*\\/\rvar __extends/;' {} \\;",
  "test": "npm run test:silent",
  "posttest": "npm run coverage:silent",
  "coverage": "cd ./coverage && istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
  "coverage:silent": "npm run --silent coverage || echo '\n\n'",
  "bench": "npm run --silent compile:es5 && node es5/test/store.bench.js",
  "docs": "typedoc --module commonjs --out ./docs/ src/**/*.ts",
  "docs:git": "git commit -am \"typedoc update\"",
  "docs:commit": "npm run docs && npm run docs:git",
  "dev:base": "clear && npm run --silent compile:test && npm run --silent dev:cover || npm run --silent dev:test || npm run --silent lint:silent || npm run --silent compile:test",
  "dev:test": "npm run --silent test:silent",
  "dev:cover": "npm run --silent test:cover && npm run --silent coverage:silent",
  "dev:temp": "clear && npm run compile:test && npm run lint && npm run test && npm run test:cover && npm run coverage",
  "dev": "chokidar 'src/**/*.ts' 'test/**/*.ts' -c 'npm run --silent dev:base'",
  "de2v": "chokidar 'src/**/*.ts' 'test/**/*.ts' -c 'npm run --silent dev:base && npm run --silent dev:cover || npm run --silent dev:test'"
},


"files": [
  "**/*.js",
  "**/*.js.map",
  "!**/*_test.js",
  "!**/*_test.js.map",
  "!es5/**/*.js",
  "!es6/**/*.js",
  "!coverage/**/*.js",
  "!node_modules/**/*.js"
],
